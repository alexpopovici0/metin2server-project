end ,make_dungeon= function ()local setting = flame_dungeon . setting ( ) 
d . new_jump_party ( 351 , setting . dungeon_entry_pos [ 1 ] , setting . dungeon_entry_pos [ 2 ] ) 
d . regen_file ( "data/dungeon/flame_dungeon/npc.txt" ) 
d . setf ( "level" , 0 ) 
